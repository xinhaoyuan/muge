(with (map budan dango)
	  (set! map ("GetMap" "data/map1"))
	  (set! budan ("GetSimpleSprite" "data/budan.sprite"))
	  (set! dango ("GetSimpleSprite" "data/dango.sprite"))
	  ("AddSpriteToMap" map budan 32 0 0 32 32 31 31)	  
	  ("SpriteMoveTo" ("AddSpriteToMap" map dango 0  0 0 32 32 31 31) 200 30 0 120)
	  ("SetCurrentMap" map)
	  ("SetViewPoint" 0 0)
	  )
