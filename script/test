(with (map budan dango)
	  (set! map ("GetMap" "data/map1"))
	  (set! budan ("GetSimpleSprite" "data/budan.sprite"))
	  (set! dango ("GetSimpleSprite" "data/dango.sprite"))
	  ("AddSpriteToMap" map budan 48 16 0 32 32 31 31)
	  ("AddSpriteToMap" map dango 16 16 0 32 32 31 31)
	  ("SetCurrentMap" map)
	  ("SetViewPoint" -320 -200)
	  )
